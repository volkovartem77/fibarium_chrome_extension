{
  "short_name": "Your Extension Name",
  "name": "Your Extension Name",
  "icons": {
    "16": "favicon.ico",
    "48": "logo192.png",
    "128": "logo192.png"
  },
  "permissions": [
    "activeTab",
    "storage",
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["./static/js/main.js"]
    }
  ],
  "manifest_version": 2,
  "version": "0.0.1",
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Your Extension Name"
  }
}